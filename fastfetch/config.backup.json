{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "type": "none"
        //"type": "auto",
        //"source": ".config/fastfetch/void.txt"
    },
    
    "display": {
        "separator": " "
    },
    "modules": [
        {
            "key": "   {#32}array{#39}@{#32}main-nix",
