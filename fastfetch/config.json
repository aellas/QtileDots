{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "type": "small"
    },
    "display": {
        "separator": ""
    },
    "modules": [
        {
            "key": "{#34}~ fastfetch",
            "type": "custom"
        },
        {
            "key": "{#36}OS  ",
            "type": "os",
            "keyColor": "white"
        },
        {
            "key": "{#36}KER ",
            "type": "kernel",
            "keyColor": "white"
        },
        {
            "key": "{#36}WM  ",
            "type": "wm",
            "keyColor": "white"
        },
        {
            "key": "{#36}PKG ",
            "type": "packages",
            "keyColor": "white"
        },
        {
            "key": "{#36}UP  ",
            "type": "uptime",
            "keyColor": "white"
        },
        {
            "type": "command",
            "key": "{#36}DAY ",
            "text": "bash ~/.config/fastfetch/days.sh",
            "keyColor": "white"
        },

        {
            "key": "\n",
            "type": "custom"
        }
    ]
}
