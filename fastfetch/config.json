{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "type": "none"
    },
    
    "display": {
        "separator": ""
    },
    "modules": [
        {
            "key": " ~ » {#32}array{#39}@{#32}main-void",
            "type": "custom"
        },
        {
            "key": " ───────────────────────────────────",
            "type": "custom"
        },
        {
            "key": " {#36}os   ",
            "type": "os",
            "keyColor": "white"
        },
        {
            "key": " {#36}ker  ",
            "type": "kernel",
            "keyColor": "white"
        },
        {
            "key": " {#36}wm   ",
            "type": "wm",
            "keyColor": "white"
        },
        {
            "key": " {#36}pkg  ",
            "type": "packages",
            "keyColor": "white"
        },
        {
            "key": " {#36}up   ",
            "type": "uptime",
            "keyColor": "white"
        },
        {
            "key": " ───────────────────────────────────",
            "type": "custom"
        },
	{
            "type": "host",
	    "key": " {#36}hst  "


	},
        {
            "type": "cpu",
            "key": " {#36}cpu  ",
            "format": "{name}"
        },
        {
            "key": " {#36}gpu  ",
            "type": "gpu",
            "keyColor": "white"
        },
        {
            "key": " {#36}ram  ",
            "type": "memory",
            "keyColor": "white"
        },
        {
            "key": " ───────────────────────────────────",
            "type": "custom"
        },
        {
            "key": " ~ » {#32}2 YEAR CHALLENGE",
            "type": "custom"
        },
        {
            "type": "command",
            "key": " {#36}day  ",
            "text": "bash ~/.config/fastfetch/days.sh",
            "keyColor": "white"
        },
        {
            "type": "command",
            "key": " {#36}lft  ",
            "text": "bash ~/.config/fastfetch/days-left.sh",
            "keyColor": "white"
        },
        {
            "type": "command",
            "key": " {#36}pct  ",
            "text": "bash ~/.config/fastfetch/progress.sh",
            "keyColor": "white"
        },
        {
            "key": "\n",
            "type": "custom",
            "keyColor": "white"
        }

    ]
}
